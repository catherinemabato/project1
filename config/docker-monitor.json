{
	"plugin": "journald",
	"pluginConfig": {
		"source": "docker"
	},
	"logPath": "/var/log/journal",
	"lookback": "5m",
	"bufferSize": 10,
	"source": "docker-monitor",
	"conditions": [],
	"rules": [
		{
			"type": "permanent",
			"reason": "CorruptDockerImage",
			"pattern": "container start failed: ErrImagePull: (.+): directory not empty"
		}
	]
}
